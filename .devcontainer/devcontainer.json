{
  "name": "Contoso Outdoors",
  "dockerComposeFile": "../docker-compose.yml",
  "service": "web",
  "workspaceFolder": "/app",
  "runArgs": [
    "--userns=keep-id"
  ],
  "forwardPorts": [
    3000
  ],
  "portsAttributes": {
    "3000": {
      "label": "Application",
      "onAutoForward": "openPreview"
    }
  },
  "postCreateCommand": "npm --prefix apps/web install"
}
